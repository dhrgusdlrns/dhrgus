msgctxt "[0201B866]"
msgid "Another time, perhaps?"
msgstr ""

msgctxt "[0201B867]"
msgid "As you say."
msgstr "당신 말대로."

msgctxt "[0201B8AE]"
msgid "Pardon my reach..."
