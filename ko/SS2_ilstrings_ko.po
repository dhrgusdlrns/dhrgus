#. ScriptNotes = Happy
#. VoiceType = SS2_WC_MontyVT
#. Subtype = Greeting
msgctxt "[0201FFD3]"
msgid "Ah, mine own Brew-prentice! Here, a recipe for you."
msgstr ""

#. ScriptNotes = Happy
#. VoiceType = SS2_WC_MontyVT
#. Subtype = Greeting
msgctxt "[0201FFD4]"
msgid "The time has cometh to brew! Behold, a new recipe!"
msgstr ""

#. ScriptNotes = Confident
#. VoiceType = SS2_WC_MontyVT
#. Subtype = Greeting
msgctxt "[0201FFD5]"
msgid "To brew, perchance to drink. A new recipe! For you."
msgstr ""

#. ScriptNotes = Question
#. VoiceType = SS2_WC_MontyVT
#. Subtype = Greeting
msgctxt "[0201FFD6]"
msgid "Doth thee seeketh a new recipe? Because I have one for you."
msgstr ""

#. ScriptNotes = Confident
#. VoiceType = SS2_WC_MontyVT
#. Subtype = Greeting
msgctxt "[0201FFD7]"
msgid "A recipe! Here, taketh it! Before I change my mind."
msgstr ""

#. ScriptNotes = Confident
#. VoiceType = SS2_WC_MontyVT
#. Subtype = Greeting
