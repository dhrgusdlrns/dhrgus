msgstr "그렇게 할 수 없습니다."

msgctxt "[02048C0F]"
msgid "That doesn't make sense."
msgstr ""

msgctxt "[02048C10]"
msgid "No, sorry."
msgstr "아냐, 미안해."

msgctxt "[02048C13]"
msgid "At least it's over."
msgstr ""

msgctxt "[02048C14]"
msgid "Someone should probably check the bodies for anything useful. Dibs?"
msgstr ""

msgctxt "[02048C15]"
msgid "Why do people always pick on me?"
msgstr ""

msgctxt "[02048C16]"
msgid "Humans are so violent..."
msgstr ""

msgctxt "[02048C19]"
msgid "Hiya!"
msgstr ""

msgctxt "[02048C1A]"
msgid "Eya!"
msgstr ""

msgctxt "[02048C1D]"
msgid "Hiii-ah!"
msgstr ""

msgctxt "[02048C1E]"
msgid "Yah-kaaah!"
msgstr ""

msgctxt "[02048C21]"
msgid "Uh, duck!"
msgstr ""

msgctxt "[02048C22]"
