msgctxt "[02048C04]"
msgid "Never fought an alien before, have you?"
msgstr ""

msgctxt "[020A4A9B]"
msgid "You got it."
msgstr "알았어."

msgctxt "[020A4A9C]"
msgid "Sure, no problem."
msgstr ""

msgctxt "[020A4A9D]"
msgid "Martian, away!"
msgstr ""

msgctxt "[020A4A9E]"
