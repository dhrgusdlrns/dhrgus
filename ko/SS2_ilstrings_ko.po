msgstr "좋아요."

msgctxt "SS2_Dialogue_ViceroySharedInfo01_YourLoss"
msgid "Your loss."
msgstr "손해보는 거야."

msgctxt "[0201DEEF]"
msgid "I can't do that."
msgstr "그렇게 할 수 없습니다."

msgctxt "[0201DEF0]"
msgid "No."
msgstr "아니."

msgctxt "[0201DEF1]"
msgid "Not gonna happen."
msgstr "그럴 일 없을 거야."

msgctxt "[0201DEF2]"
msgid "Impossible."
msgstr ""

msgctxt "[0201DFD0]"
msgid "Umm... sure."
msgstr ""

